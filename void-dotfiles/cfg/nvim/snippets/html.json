{
	"Website head tags": {
		"prefix": "head",
		"body": [
			"    <meta charset=\"UTF-8\" />",
			"    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />",
			"",
			"    <!-- || title -->",
			"    <title>PLACEHOLDER_TITLE</title>",
			"    <meta name=\"twitter:title\" content=\"PLACEHOLDER_TITLE\" />",
			"    <meta property=\"og:title\" content=\"PLACEHOLDER_TITLE\" />",
			"",
			"    <!-- || description - around 150 characters -->",
			"    <meta",
			"      name=\"description\"",
			"      content=\"PLACEHOLDER_DESCRIPTION\"",
			"    />",
			"    <meta",
			"      name=\"twitter:description\"",
			"      content=\"PLACEHOLDER_DESCRIPTION\"",
			"    />",
			"    <meta",
			"      property=\"og:description\"",
			"      content=\"PLACEHOLDER_DESCRIPTION\"",
			"    />",
			"",
			"    <!-- || static Facebook Open Graph tags -->",
			"    <!-- make sure og:url content doesn't start with www -->",
			"    <meta property=\"og:url\" content=\"https://www.PLACEHOLDER_SITENAME.pages.dev/\" />",
			"    <meta property=\"og:site_name\" content=\"PLACEHOLDER_SITENAME\" />",
			"",
			"    <!-- || og:image and twitter:image -->",
			"    <!-- og:image should be 1200x630 px -->",
			"    <!-- twitter:image can be any size. -->",
			"    <!-- make sure og:image and twitter:image content doesn't start with www -->",
			"    <meta property=\"og:image\" content=\"https://picsum.photos/1200/630\" />",
			"    <meta",
			"      property=\"twitter:image\"",
			"      content=\"https://picsum.photos/1200/630\"",
			"    />",
			"    <meta name=\"twitter:card\" content=\"summary_large_image\" />",
			"",
			"    <!-- || icons & webmanifest -->",
			"    <link",
			"      rel=\"apple-touch-icon\"",
			"      sizes=\"180x180\"",
			"      href=\"https://picsum.photos/180/180\"",
			"    />",
			"    <link",
			"      rel=\"icon\"",
			"      type=\"image/png\"",
			"      sizes=\"32x32\"",
			"      href=\"https://picsum.photos/32/32\"",
			"    />",
			"    <link",
			"      rel=\"icon\"",
			"      type=\"image/png\"",
			"      sizes=\"16x16\"",
			"      href=\"https://picsum.photos/16/16\"",
			"    />",
			"    <link",
			"      rel=\"manifest\"",
			"      href=\"https://oraoracalc.pages.dev/site.webmanifest\"",
			"    />"
		],
		"description": "Website head tags"
	},
	"HTML Template": {
		"prefix": "doc",
		"body": [
			"<!DOCTYPE html>",
			"<html lang=\"en\">",
			"<head>",
			"  <meta charset=\"UTF-8\">",
			"  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
			"  <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">",
			"  <title>${1:Document}</title>",
			"  <link rel=\"stylesheet\" href=\"${2:styles.css}\">",
			"</head>",
			"<body>",
			"  ${3:<!-- Content goes here -->}",
			"  <script src=\"${4:script.js}\"></script>",
			"</body>",
			"</html>"
		],
		"description": "Generate a basic HTML template"
	}
}
